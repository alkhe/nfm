@font-face
	src url('/fonts/gubblebum.ttf')
	font-family 'Gubblebum'

transition(rate)
	transition rate ease-in-out
	-webkit-transition rate ease-in-out
	-moz-transition rate ease-in-out
	-ms-transition rate ease-in-out
	-o-transition rate ease-in-out
	-khtml-transition rate ease-in-out

noselect()
	-webkit-touch-callout none
	-webkit-user-select none
	-khtml-user-select none
	-moz-user-select none
	-ms-user-select none
	user-select none

html, body
	margin 0 auto
	padding 0
	background #282828 !important;
	color #f0f0f0
	font 15px 'Source Sans Pro', sans-serif

*
	outline none
	noselect()
	transition .08s

i
	font-family 'FontAwesome' !important
	font-style normal !important

#header
	position absolute
	top 0
	left 0
	background #202020
	width 100%

#logo
	font-family 'Gubblebum', Monospace !important
	font-size 30px

#explorer
	height calc(100% - 45px) !important;
	max-height calc(100% - 45px) !important
	background #202020
	overflow auto

.row
	margin 0 auto !important
	padding 0 !important
	max-width 100% !important

.columns
	padding-left 0 !important
	padding-right 0 !important

